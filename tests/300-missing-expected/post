. ../post.functions

trap "rm -f test/*.actual test/*.diff" EXIT

assert_file test/out.actual
assert_file test/err.actual
assert_file test/exit.actual
assert_file test/stray.actual

assert_file test/out.diff
assert_file test/err.diff
assert_file test/exit.diff
assert_file test/stray.diff

assert_file test/out.diff.tmp
assert_file test/err.diff.tmp
assert_file test/exit.diff.tmp
assert_file test/stray.diff.tmp

assert_same "expected/exit.diff" "test/exit.diff"
assert_same "expected/stray.diff" "test/stray.diff"
